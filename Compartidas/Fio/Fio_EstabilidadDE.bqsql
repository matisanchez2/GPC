--     WHEN MAX_AMOUNT_DE_JULIO > MAX_AMOUNT_DE_JUNIO * 1.3 THEN 'LE DI MAS'
--     ELSE 'MANTENGO OFERTA' END AS JULIO_VS_JUNIO

FROM ESTABILIDAD_FINAL_CUANTIFICACIONES_V2 WHERE APAGADOS='1.SIEMPRE PRENDIDO'

